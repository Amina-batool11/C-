#include<iostream>
using namespace std;
class Animal
{
private:
void Eat()
{
cout<<"eating"<<endl;
}
public:

void Sleep()
{
cout<<"sleeping"<<endl;
}
protected:

void Run()
{
cout << "running" << endl;
}
};
class Dog:public Animal
{
public:
void Bark()
{
cout << "Barking" << endl;
}
};
int main()
{
Dog d;
d.Eat();
d.Sleep();
d.Run();
d.Bark();
}

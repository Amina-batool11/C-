#include<iostream>
using namespace std;
class Base
{public:
virtual void show ()=0;

};
class Derv1:public Base
{
	public:
	void show() override
	{
	cout<<"show picture"<<endl;
}
};
class Derv2:public Base
{public:
	void show() override
	{
		cout<<"show all pictures"<<endl;
	}
};
int main()
{ 
Derv1 d1;
Derv2 d2;
d1.show();

d2.show();

}

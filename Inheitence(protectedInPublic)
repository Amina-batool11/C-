#include<iostream>
using namespace std;
class Teacher
{ 
 private:
 void ID()
  {
    cout<<"99999"<<endl;
  }
  public:
 void Experience()
  {
     cout << "20 years" << endl;
  }
  protected:
  void Salary()
  {
  cout << "Salary is..." << endl;
  }
};
class Student: public Teacher 
{
  private:
  void age()
  {
    cout << "age is...." << endl;
  }
  public:
  void rollnumber()
  {
    cout << "roll number is..." << endl;
  }
  protected:
  void address()
  {
  cout << "address is..." << endl;
  }
}; 
class me: public Student
{
private:
void CNIC()
{
cout << "CNIC Num..." << endl;
}
public:
void Marks()
{
cout << "Marks are..." << endl;

address ();
}
};
int main()
{
Student s;
Teacher t;
me m;
t.Experience();

m.rollnumber();
s.rollnumber();
m.Marks();
}
